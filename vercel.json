{
    "version": 2,
    "builds": [
      {
        "src": "run.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/templates/(.*)",
        "dest": "/templates/$1"
      },
      {
        "src": "/",
        "dest": "run.py"
      },
      {
        "src": "/blog",
        "dest": "run.py"
      },
      {
        "src": "/admin",
        "dest": "run.py"
      }
    ]
  }
  